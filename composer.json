{
    "name": "cedric-lekene/laravel-migration-ai",
    "description": "A Laravel package that uses AI to generate database migrations from plain-language descriptions, simplifying the migration creation process",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LekeneCedric",
            "email": "lekenelucprofessionel@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CedricLekene\\LaravelMigrationAI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CedricLekene\\LaravelMigrationAI\\LaravelMigrationAIServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.52.16|^10.0|^11.0",
        "illuminate/support": "^9.52.16|^10.0|^11.0",
        "illuminate/database": "^9.21|^10.0"
    }
}
